{
  "name": "seidmann-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/ensure-admin.js && next dev",
    "clean": "node scripts/clean-next.js",
    "dev:clean": "node scripts/clean-next.js && node scripts/ensure-admin.js && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css}\"",
    "check-prisma": "node scripts/check-prisma-queries.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:clean": "node scripts/clean-prisma-windows.js",
    "db:seed": "node prisma/seed.cjs",
    "prepare": "husky",
    "pre-commit": "lint-staged"
  },
  "prisma": {
    "seed": "node prisma/seed.cjs"
  },
  "dependencies": {
    "@prisma/client": "6.19.2",
    "@types/archiver": "^7.0.0",
    "archiver": "^7.0.1",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.0.0",
    "jose": "^5.10.0",
    "lucide-react": "^0.562.0",
    "next": "14.0.4",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.1",
    "pdfjs-dist": "^5.3.93",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.10.3",
    "tailwind-merge": "^2.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-next": "14.0.4",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.32",
    "prettier": "^3.2.5",
    "prisma": "6.19.2",
    "tailwindcss": "^3.3.6",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
